        "    \"cp\": [2],          # chest pain type (encoded)\n",
        "    \"trestbps\": [140],  # resting blood pressure\n",
        "    \"chol\": [250],      # serum cholesterol\n",
        "    \"fbs\": [0],         # fasting blood sugar > 120 mg/dl\n",
